{block content}

<section class="homepage-slide js-movingGradient">
	<div class="container-fluid">
		<div class="inside">
			<div class="text">
				<h1 n:block=title>{if $locale == 'cs'}<img src="/gfx/headline.svg" alt="{_translation.homepage.slide.headline|noescape}" style="width: 50vw;">{/if}{if $locale == 'en'}<img src="/gfx/headline_en.svg" alt="{_translation.homepage.slide.headline|noescape}" style="width: 50vw;">{/if}</h1>
				<p>{_translation.homepage.slide.text|noescape}</p>
				<a class="btn btn-orange btn-big" n:href="Page:klubKlicovychZakaznikuPlzenskyPrazdroj">
					<span class="btn__text">{_translation.homepage.slide.link}</span>
					<svg class="btn__icon" xmlns="http://www.w3.org/2000/svg" width="7" height="12" viewBox="0 0 39.85 73.57">
					<polygon points="4.55 73.57 0 68.78 30.79 36.79 0 4.79 4.55 0 39.85 36.73 39.79 36.79 39.85 36.84 4.55 73.57" style="fill: currentColor;fill-rule: evenodd; stroke: currentColor; stroke-width: 2px"/>
					</svg>
				</a><br>
				<!--<a n:href="Page:clanky, '2'">{_translation.homepage.slide.videoLink}</a>-->
                <p class="responsive-text">
                    {_translation.header.info.sect1} <a n:href="Page:vytvarimeLoga">{_translation.header.info.sect2}</a>, <a n:href="Page:vytvarimeWeby">{_translation.header.info.sect3}</a> {_translation.header.info.sect4} <a n:href="Page:vytvarimeObaly">{_translation.header.info.sect5}</a>, <a n:href="Page:nasePrace, 'filter' => '4'">{_translation.header.info.sect6}</a> {_translation.header.info.sect7} <a n:href="Page:vytvarimePrezentace">{_translation.header.info.sect8}</a>.
                </p>
			</div>
			<div class="ddd-logo">
				<img src="/gfx/logo-ddd.svg" alt="Dost dobrÃ½ design - logo">
			</div>
		<!--<a href="#first-project" class="scroll-to jumping-arrow-homepage"><span class="casestudies">{_translation.homepage.slide.situation}<span></a>-->

		</div>
	</div>
</section>


<section>
    <div class="row row-no-gap">
