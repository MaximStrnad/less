{block content}
<section class="content-slide logo-absolute" style="background-image: url(../gfx/drevosrot/ds_bg01.jpg);">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-6 text-center">
                <img src="/gfx/drevosrot/ds_logo01.svg" alt="Logo" style="height: 4.375vw; max-height: 84px;">
            </div>
            <div class="col-md-6 text-right">
                <h1 n:block=title>{_translation.ds.slide.headline|noescape}</h1>
                <p>{_translation.ds.slide.text|noescape}</p>
                <span class="highlight">{_translation.ds.slide.highlight.hig1}</span><span class="highlight">{_translation.ds.slide.highlight.hig2}</span><span class="highlight">{_translation.ds.slide.highlight.hig3}</span><span class="highlight">{_translation.ds.slide.highlight.hig4}</span>
            </div>
        </div>
        <div class="ddd-logo">
          	<img src="/gfx/drevosrot/ddd_logo.svg" alt="Logo" class="svg-icon">
        </div>
<!--        <a href="#fisrt-section" class="scroll-to jumping-arrow ds-arrow"></a>-->
    </div>
</section>
<section class="text-section bg-primary" id="fisrt-section">
    <div class="container-fluid">
        <div class="headline-box p-only">
            <p>{_translation.ds.green.topText|noescape}</p>
        </div>
        <div class="row cols-4">
            <div class="col-md-6 col-md-offset-3">
                <div class="row text-center">
                    <div class="col-md-4">
                        <span>{_translation.ds.green.row.col1.above}</span>
                        <h3>{_translation.ds.green.row.col1.headline}</h3>
                        <p>{_translation.ds.green.row.col1.text|noescape}</p>
                    </div>
                    <div class="col-md-4">
                        <span>{_translation.ds.green.row.col2.above}</span>
                        <h3>{_translation.ds.green.row.col2.headline}</h3>
                        <p>{_translation.ds.green.row.col2.text|noescape}</p>
                    </div>
                    <div class="col-md-4">
                        <span>{_translation.ds.green.row.col3.above}</span>
                        <h3>{_translation.ds.green.row.col3.headline}</h3>
                        <p>{_translation.ds.green.row.col3.text|noescape}</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section>
    <div class="cycle-slideshow"
         data-cycle-fx="scrollHorz"
         data-swipe="true"
         data-cycle-swipe-fx="scrollHorz">
        <img src="/gfx/drevosrot/ds_pf.jpg" alt="">
        <img src="/gfx/drevosrot/ds_letak.jpg" alt="">
        <img src="/gfx/drevosrot/ds_vizitka.jpg" alt="">
        <img src="/gfx/drevosrot/ds_logo.jpg" alt="">
        <span class="cycle-prev"></span>
        <span class="cycle-next"></span>
    </div>
</section>



<section class="text-section bg-primary-small text-section-big">
    <div class="container-fluid">
        <div class="text-right">
            <h2>{_translation.ds.greenSmall.headline|noescape}</h2>
            <p><strong>Dominika Vizváryová</strong><br>{_translation.destro.greenSmall.position}, Dřevošrot a. s<br><a href="https://www.linkedin.com/in/dominika-vizv%C3%A1ryov%C3%A1-b8b5126a/" class="linkedIn" target="_blank"></a></p>
        </div>
    </div>
</section>

