<section class="text-section bg-white show-logos">
    <div class="container-fluid">
        <div class="row text-center kkz">
            <div class="col-md-6">
                <div class="img-wrap">
                    <img src="/gfx/drevosrot/ds_logo-old.png" alt="" style="height: 2.0312vw; max-height: 39px;">
                </div>
                <span>{_translation.ds.white.logoOld}</span>
            </div>
            <div class="col-md-6">
                <div class="img-wrap">
                    <img src="/gfx/drevosrot/ds_logo02.svg" alt="" style="height: 4.3750vw; max-height: 84px;">
                </div>
                <span>{_translation.ds.white.logoNew}</span>
            </div>
        </div>
    </div>
</section>


<section class="show-website" style="background-image: url(../gfx/drevosrot/ds_bg01.jpg);">
    <div class="container-fluid">
        <div class="wrapper">
            <div class="desktop">
                <div class="website paralax-website" style="background-image: url(../gfx/drevosrot/ds_web.jpg)"></div>
            </div>
            <div class="phone">
                <div class="website paralax-website" style="background-image: url(../gfx/drevosrot/ds_mobil.jpg)"></div>
            </div>
            <div class="link">
                <a class="arrow-right" href="http://www.drevosrot.cz/" target="_blank">{_translation.showWebsite}</a>
            </div>
        </div>
    </div>
</section>











