<div class="loader-next-page">
    <section class="content-slide slide-dark" style="background-image: url(../gfx/kkzpp/kkzpp_bg01.jpg);" data-next-page="{link Page:klubKlicovychZakaznikuPlzenskyPrazdroj}">
        <div class="overlay"></div>
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-6 text-center">
                    <img src="/gfx/kkzpp/kkzpp_logo01.png" alt="Logo" style="height: 9.375vw; max-height: 180px;">
                </div>
                <div class="col-md-6 text-right">
                    <h2>{_translation.footer.scrollForNext}</h2>
                    <span class="percent">0</span>
